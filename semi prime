from math import *
def semiprime(n):
    ran=sqrt(n)
    i=2
    while(i<=ran):
        c=0
        if(n%i==0):
            for j in range(2,i):
                if(i%j==0):
                    c=1
            if(c==0):
                for k in range(2,n//i):
                    if((n//i)%k==0):
                        c=1
            if(c==0):
                return True
        i+=1
    return False
n=int(input())
print(semiprime(n))
